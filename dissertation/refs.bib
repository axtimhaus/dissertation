@article{Johnson1964,
    title = {Grain Boundary and Volume Diffusion in the Sintering of Silver},
    author = {Johnson, D. Lynn and Glarke, T. M},
    date = {1964-10-01},
    journaltitle = {Acta Metallurgica},
    shortjournal = {Acta Metallurgica},
