    shorttitle = {Determination of {{Pipe Diffusion Coefficients}} in {{Undoped}} and {{Magnesia-Doped Sapphire}} (α-{{Al2O3}})},
    author = {Tang, Xiaohong and Lagerlöf, K. Peter D. and Heuer, Arthur H.},
