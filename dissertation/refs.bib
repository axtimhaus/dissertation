@article{Kaysser1984,
    title = {Particle Growth by Coalescence during Liquid Phase Sintering of {{Fe-Cu}}},
    author = {Kaysser, W. A. and Takajo, S. and Petzow, G.},
    date = {1984-01-01},
    journaltitle = {Acta Metallurgica},
